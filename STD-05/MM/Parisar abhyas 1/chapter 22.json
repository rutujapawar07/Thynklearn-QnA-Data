    { "question_id": 49, "question_type": "MCQ", "question": "स्वभावातील चांगुलपणा कशावर परिणाम करतो?", "options": [{ "option_id": 1, "text": "मैत्री आणि नात्यांवर", "is_correct": true }, { "option_id": 2, "text": "उंचीवर", "is_correct": false }], "answer": "मैत्री आणि नात्यांवर" },
    { "question_id": 50, "question_type": "MCQ", "question": "आपली स्वतःची ओळख कशामुळे निर्माण होते?", "options": [{ "option_id": 1, "text": "कर्तृत्व आणि व्यक्तिमत्त्वामुळे", "is_correct": true }, { "option_id": 2, "text": "केवळ नावाने", "is_correct": false }], "answer": "कर्तृत्व आणि व्यक्तिमत्त्वामुळे" },

    { "question_id": 51, "question_type": "SHORT", "question": "वाढ म्हणजे काय?", "answer": "वयानुसार शरीराची उंची आणि वजन वाढणे याला वाढ म्हणतात." },
    { "question_id": 52, "question_type": "SHORT", "question": "कौशल्य म्हणजे काय?", "answer": "एखादे काम करण्याची क्षमता किंवा पद्धत आत्मसात करणे म्हणजे कौशल्य होय." },
    { "question_id": 53, "question_type": "SHORT", "question": "व्यक्तिमत्त्व म्हणजे काय?", "answer": "एखाद्या व्यक्तीचे शारीरिक रूप, बुद्धी, कौशल्ये आणि स्वभाव यांचा एकत्रित परिणाम म्हणजे व्यक्तिमत्त्व होय." },
    { "question_id": 54, "question_type": "SHORT", "question": "आनुवंशिकता म्हणजे काय?", "answer": "पालकांची काही शारीरिक किंवा मानसिक वैशिष्ट्ये मुलांमध्ये उतरण्याच्या प्रक्रियेला आनुवंशिकता म्हणतात." },
    { "question_id": 55, "question_type": "SHORT", "question": "बाळाचे पहिले कौशल्य कोणते?", "answer": "मान धरणे किंवा आईला ओळखून हसणे हे बाळाचे पहिले कौशल्य असू शकते." },
    { "question_id": 56, "question_type": "SHORT", "question": "विकास म्हणजे काय?", "answer": "वयानुसार नवीन कौशल्ये शिकणे आणि स्वतःला सक्षम बनवणे याला विकास म्हणतात." },
    { "question_id": 57, "question_type": "SHORT", "question": "वाढ कोणत्या वयापर्यंत होते?", "answer": "मानवी वाढ साधारणपणे १८ ते २० वर्षांपर्यंत होते." },
    { "question_id": 58, "question_type": "SHORT", "question": "कौशल्ये कशामुळे प्राप्त होतात?", "answer": "कौशल्ये शिक्षण, सराव आणि मार्गदर्शनामुळे प्राप्त होतात." },
    { "question_id": 59, "question_type": "SHORT", "question": "शारीरिक वाढीसाठी काय आवश्यक आहे?", "answer": "पौष्टिक आहार, पुरेशी झोप आणि नियमित व्यायाम आवश्यक आहे." },
