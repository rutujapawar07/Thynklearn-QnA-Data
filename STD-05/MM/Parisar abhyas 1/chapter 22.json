    { "question_id": 88, "question_type": "SHORT", "question": "प्राण्यांमध्ये कौशल्ये असतात का?", "answer": "हो, पण मानवामध्ये ती अधिक प्रगत स्वरूपात असतात." },
    { "question_id": 89, "question_type": "SHORT", "question": "आत्मविश्वास कशामुळे येतो?", "answer": "ज्ञान आणि कौशल्यांमुळे आत्मविश्वास येतो." },
    { "question_id": 90, "question_type": "SHORT", "question": "चांगल्या मित्रांचे महत्त्व काय?", "answer": "चांगले मित्र आपल्याला चांगली कौशल्ये शिकायला मदत करतात." },
    { "question_id": 91, "question_type": "SHORT", "question": "बाळाचे वजन जन्मानंतर वाढते का?", "answer": "हो, बाळाचे वजन वेगाने वाढते." },
    { "question_id": 92, "question_type": "SHORT", "question": "कोणता आहार टाळावा?", "answer": "फास्ट फूड आणि जास्त साखर असलेले पदार्थ टाळावेत." },
    { "question_id": 93, "question_type": "SHORT", "question": "एकाग्रता कशी वाढवावी?", "answer": "ध्यान आणि आवडीच्या कामात लक्ष देऊन." },
    { "question_id": 94, "question_type": "SHORT", "question": "सामाजिक विकास म्हणजे काय?", "answer": "समाजात इतरांशी मिळूनमिसळून वागणे म्हणजे सामाजिक विकास." },
