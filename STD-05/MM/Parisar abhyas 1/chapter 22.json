    { "question_id": 96, "question_type": "SHORT", "question": "कौशल्यामुळे काय वाचते?", "answer": "कौशल्यामुळे वेळ आणि श्रम वाचतात." },
    { "question_id": 97, "question_type": "SHORT", "question": "भावनिक संतुलन का हवे?", "answer": "योग्य निर्णय घेण्यासाठी भावनिक संतुलन हवे." },
    { "question_id": 98, "question_type": "SHORT", "question": "वाढ मोजण्यासाठी कशाचा तक्ता वापरतात?", "answer": "वयोगट आणि उंचीचा तक्ता." },
    { "question_id": 99, "question_type": "SHORT", "question": "अन्न शिजवणे हे काय आहे?", "answer": "हे एक शिकलेले कौशल्य आहे." },
    { "question_id": 100, "question_type": "SHORT", "question": "बालपण म्हणजे काय?", "answer": "जन्मापासून साधारणतः १२-१३ वर्षांपर्यंतचा काळ." },
    { "question_id": 101, "question_type": "SHORT", "question": "प्रौढावस्था म्हणजे काय?", "answer": "१८ वर्षांनंतरचा शारीरिकदृष्ट्या परिपक्व काळ." },
    { "question_id": 102, "question_type": "SHORT", "question": "स्मरणशक्तीचे महत्त्व सांगा.", "answer": "शिकलेल्या गोष्टी लक्षात ठेवण्यासाठी." },
    { "question_id": 103, "question_type": "SHORT", "question": "स्वच्छता आरोग्याशी कशी संबंधित आहे?", "answer": "स्वच्छतेमुळे जंतूंचा प्रसार थांबतो आणि आपण आजारी पडत नाही." },
    { "question_id": 104, "question_type": "SHORT", "question": "क्रीडा प्रकारांचा फायदा काय?", "answer": "शारीरिक हालचाल आणि टीमवर्क शिकता येते." },
    { "question_id": 105, "question_type": "SHORT", "question": "कौतुक केल्याने काय होते?", "answer": "उत्साह वाढतो आणि नवीन शिकण्याची प्रेरणा मिळते." },
    { "question_id": 106, "question_type": "SHORT", "question": "अपयशातून काय शिकावे?", "answer": "अपयशातून पुन्हा प्रयत्न करण्याची जिद्द शिकावी." },
    { "question_id": 107, "question_type": "SHORT", "question": "व्यक्तिमत्त्व आणि कपडे यांचा संबंध आहे का?", "answer": "कपड्यांमुळे बाह्य रूप दिसते, पण आतील गुण खरे व्यक्तिमत्त्व ठरवतात." },
    { "question_id": 108, "question_type": "SHORT", "question": "प्रभावी संवाद म्हणजे काय?", "answer": "आपले विचार इतरांना नीट समजावून सांगणे." },
    { "question_id": 109, "question_type": "SHORT", "question": "सतत शिकत राहणे का गरजेचे आहे?", "answer": "बदलत्या जगासोबत राहण्यासाठी." },
    { "question_id": 110, "question_type": "SHORT", "question": "वाढ खुंटण्याची कारणे सांगा.", "answer": "आजारपण, अपुरा आहार आणि व्यायामाचा अभाव." },
    { "question_id": 111, "question_type": "SHORT", "question": "जिद्द म्हणजे काय?", "answer": "कठोर परिश्रमाने ध्येय गाठण्याची तयारी." },
    { "question_id": 112, "question_type": "SHORT", "question": "मोठ्यांचा मान का राखावा?", "answer": "त्यांच्या अनुभवातून आपल्याला शिकायला मिळते म्हणून." },
    { "question_id": 113, "question_type": "SHORT", "question": "वेळेवर झोपण्याचे फायदे काय?", "answer": "शरीराला विश्रांती मिळते आणि मेंदू ताजेतवाना होतो." },
    { "question_id": 114, "question_type": "SHORT", "question": "आपल्या आवडी कशा विकसित होतात?", "answer": "नवनवीन गोष्टींच्या संपर्कात आल्यावर." },
